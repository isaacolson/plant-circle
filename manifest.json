 "version": "1.0.1",
 "min_os_version_major": 6,
 "url": "https://raw.githubusercontent.com/isaacolson/plant-circle/master/manifest.json",
 "zip": "https://github.com/isaacolson/plant-circle/archive/master.zip",
 "executable": "python",
 "args": ["plant-circle-master/plant_circle.py"],
 "config": [
   {"name": "x_pos", "label": "X Position", "value": 500 },
   {"name": "y_pos", "label": "Y Position", "value": 500 },
   {"name": "diameter", "label": "Circle Diameter", "value": 500 },
   {"name": "min_dist", "label": "Distance Apart (keep above 50)", "value": 150 },
   {"name": "size", "label": "Plant Size", "value": 25 },
   {"name": "name", "label": "Plant Name (what Farm Designer will call it.)", "value": "Circle Plant" },
